  
  <%= render "devise/shared/error_messages", resource: resource %>
  <section id="logon" style="background-image: url('/assets/Logon.jpg')">
    <div class="contain">
      <div class="log_blk">
        <div class="form_wrapper">
          <div class="logo">
            <img src="/assets/logo-on.svg" alt="">
          </div>
          <h3 class="mb-0">Sign in to manage</h3>
          <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
            <h5>Email Address</h5>
            <div class="form_blk error">
              <%= f.email_field :email, autofocus: true, autocomplete: "email", placeholder: "Type here", class: "input" %>
            </div>
            <div class="d-flex justify-content-between">
              <h5>Password</h5>
              <p><a href="<%=new_admin_password_path%>">Forgot password?</a></p>
            </div>
            <div class="form_blk input error">
              <img src="/assets/icon-lock-blue.svg" alt="">
              <%= f.password_field :password, autocomplete: "current-password" , placeholder: "Type here", class: "input" %>
            </div>
            <div class="btn_blk">
              <button type="submit" class="site_btn w-100">Sign in</button>
            </div>
          <%end%>
        </div>
      </div>
    </div>
    <div class="car_img">
      <img src="/assets/bg-car.png" alt="">
    </div>
  </section>
  <!-- Logon Section -->


  <!-- Main Js -->
  <script src="/assets/main.js"></script>
