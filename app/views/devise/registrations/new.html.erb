  <section id="logon" style="background-image: url(<%= asset_path "Logon.jpg" %>)">
  <div class="contain">
  <div class="log_blk">
    <div class="form_wrapper">
      <div class="logo">
        <%= image_tag "logo-on.svg" %>
      </div>
      <h3 class="mb-0">Sign up to manage</h3>
      <%= render "devise/shared/error_messages", resource: resource %>
      <p style="position:absolute; left:1%; font-weight:600; margin-top:10px; color:red;"><%= alert %></p>
      <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
        <h5>Full Name</h5>
        <div class="form_blk">
          <%= f.text_field :full_name, autofocus: true, autocomplete: "email", placeholder: "Type here", class: "input" %>
        </div>
        <h5>Email Address</h5>
        <div class="form_blk">
          <%= f.email_field :email, autofocus: true, autocomplete: "email", placeholder: "Type here", class: "input" %>
        </div>
          <h5>Password</h5>
        <div class="form_blk input">
          <%= image_tag "icon-lock-blue.svg" %>
          <%= f.password_field :password, autocomplete: "new-password", class: "input" %>
        </div>
        <div class="btn_blk pop_btn">
          <button type="submit" class="site_btn w-100">Sign Up</button>
        </div>
      <%end%>
    </div>
  </div>
  </div>
  <div class="car_img">
    <%= image_tag "bg-car.png" %>
  </div>
  
  </section>
  <!-- Logon Section -->

  <!-- Main Js -->
  <script src="/assets/main.js"></script>
