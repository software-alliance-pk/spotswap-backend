<div id="error-message">
<p style="position:absolute; left:5%; font-weight:600; margin-top:130px;"><%= notice %></p>
	
</div>
<div id="notice-message"><p style="position:absolute; left:7%; font-weight:600; margin-top:55px;"><%= alert %></p></div>


<script type="text/javascript">
	setTimeout(function() {
  $('#error-message').hide();
}, 5000);

setTimeout(function() {
  $('#notice-message').hide();
}, 5000);

</script>



